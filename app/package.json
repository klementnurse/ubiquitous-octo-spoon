{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build:production": "BUILD_PATH='./dist-production' PUBLIC_URL=https://ds2r9mr2r4h38.cloudfront.net REACT_APP_TARGET_ENV=production react-scripts build",
    "build:staging": "BUILD_PATH='./dist-staging' PUBLIC_URL=https://d1warlphuptd9z.cloudfront.net REACT_APP_TARGET_ENV=staging react-scripts build",
    "build:dev": "BUILD_PATH='./dist-development' REACT_APP_TARGET_ENV=development react-scripts build",
    "prepare:canary": "cp ./dist-production/index.html ./dist-production/next.html && shx sed -i \"s/ds2r9mr2r4h38/d1wqlk9q0j0xz7/g\" ./dist-production/next.html",
    "deploy": "yarn deploy:sync && yarn deploy:cache && yarn deploy:invalidate",
    "deploy:sync": "aws s3 sync ./dist-$ENVIRONMENT s3://$S3_BUCKET",
    "deploy:cache": "aws s3 cp s3://$S3_BUCKET s3://$S3_BUCKET --exclude 'index.html' --cache-control 'max-age=604800' --metadata-directive REPLACE --acl public-read --recursive",
    "deploy:invalidate": "aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DISTRIBUTION_ID --paths \"/*\"",
    "audit:ci": "yarn audit-ci --high -p yarn --report-type important --skip-dev",
    "audit:production:ci": "yarn audit:ci || (yarn run audit:production:vulnerability:detected && exit 1)",
    "audit:production:vulnerability:detected": "./notify_audit_error.sh",
    "clean": "rm -rf dist-*/*",
    "lint": "eslint src",
    "start": "REACT_APP_TARGET_ENV=development react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "author": "optizmo",
  "license": "private",
